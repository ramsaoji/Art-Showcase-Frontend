  "compilerOptions": {
    "target": "es2022",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true
  },
  "include": ["./**/*.ts", "migrateFirebaseData.ts"],
  "exclude": ["node_modules"]
}
